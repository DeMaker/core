{
    "name": "fojuth/stamp",
    "description": "Cascade class generation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Fojuth",
            "email": "fojuth@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "3.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "Fojuth\\Stamp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    }
}
